[alias]
    br = branch
    ci = commit
    cia = commit --all
    cim = commit --amend
    co = checkout
    dfl = diff --name-status
    dfs = diff --staged
    dl = diff HEAD^
    f = fetch
    g = grep -in
    last = log -1 HEAD
    load = stash pop
    logg = log --graph --decorate --oneline --abbrev-commit --all --no-merges
    logo = log --all --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    logs = log --all --graph --no-merges --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    mgf = merge --ff-only
    p = push
    pf = push -f
    puo = push -u origin
    rb = rebase
    rbc = rebase --continue
    rh = reset --hard
    rp = remote prune
    save = stash
    st = status
[core]
    editor = vim
    pager = less -F -X
[color]
    ui = true
[merge]
    tool = vimdiff
[push]
    default = simple
[credential]
    helper = cache --timeout 36000
[user]
    name = Lirian Su
    email = liriansu@gmail.com
